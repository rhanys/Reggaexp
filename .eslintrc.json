{
	"env": {
		"browser": true,
		"commonjs": true,
		"es6": true
	},
	"extends": ["google", "eslint:recommended", "plugin:react/recommended"],
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"ecmaVersion": 2018
	},
	"rules": {
		"indent": [2, "tab"],
		"no-tabs": 0,
		"arrow-parens": 0,
		"require-jsdoc": 0
	}
}
